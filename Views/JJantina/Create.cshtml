@model SUMBER.Models.Modules.JJantina

@{
    ViewData["Title"] = "Jantina - Tambah";
}

<h4>Jantina - Tambah</h4>

<hr />
<div class="row">
    <div class="col-md-4">
        <form asp-action="Create">
            <div asp-validation-summary="ModelOnly" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="Perihal" class="control-label"></label>
                <input asp-for="Perihal" class="form-control form-control-sm" />
                <span asp-validation-for="Perihal" class="text-danger"></span>
            </div>
            <div class="form-group">
                <input type="submit" value="Simpan" class="btn btn-primary btn-sm" />
            </div>
        </form>
    </div>
</div>

<div>
    <a asp-action="Index">Kembali ke Indeks</a>
</div>

